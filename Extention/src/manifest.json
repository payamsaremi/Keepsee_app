{
  "manifest_version": 3,
  "key": "",
  "name": "Kip",
  "description": "Kip --- keep and manage your tabs, tasks and thoughts safe, organized and always accessible",
  "options_page": "options.html",
  "background": { "service_worker": "background.bundle.js" },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon-34.png"
  },
  "permissions": ["tabs","sessions","pageCapture","history"],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "devtools_page": "devtools.html",
  "web_accessible_resources": [
    {
      "resources": ["content.styles.css", "icon-128.png", "icon-34.png","NewTab.html"],
      "matches": ["<all_urls>"]
    }
  ]
  
}
